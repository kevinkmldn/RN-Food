{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-native/all"
  ],
  "parser": "babel-eslint",
  "plugins": ["prettier", "react", "react-native"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  }
}
